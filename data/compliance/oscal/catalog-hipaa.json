          {
            "id": "ts-tx-1",
            "title": "Transmission Integrity Controls",
            "props": [
              {
                "name": "implementation-type",
                "value": "addressable"
              },
              {
                "name": "category",
                "value": "Transmission Security"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-tx-1_stmt",
                "name": "statement",
                "prose": "Implement security measures to ensure that electronically transmitted ePHI is not improperly modified without detection."
              },
              {
                "id": "ts-tx-1_guide",
                "name": "guidance",
                "prose": "Use TLS 1.2+ for all ePHI in transit. Implement message integrity checks (HMAC). Validate data integrity at endpoints."
              },
              {
                "id": "ts-tx-1_evidence",
                "name": "evidence",
                "prose": "TLS configuration evidence; Certificate inventory; Integrity check implementations"
              }
            ],
            "links": [
              {
