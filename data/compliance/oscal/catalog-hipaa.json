          {
            "id": "ts-pe-1",
            "title": "Person or Entity Authentication",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Authentication"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-pe-1_stmt",
                "name": "statement",
                "prose": "Implement procedures to verify the identity of a person or entity seeking access to ePHI, including multi-factor authentication."
              },
              {
                "id": "ts-pe-1_guide",
                "name": "guidance",
                "prose": "Implement MFA for all systems containing ePHI. Verify user identity before granting access. Use strong authentication methods."
              },
              {
                "id": "ts-pe-1_evidence",
                "name": "evidence",
                "prose": "MFA configurations; Authentication policies; MFA enrollment reports"
              }
