          {
            "id": "ts-ac-2",
            "title": "Emergency Access Procedure",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Access Control"
              },
              {
                "name": "automation-capability",
                "value": "partial"
              }
            ],
            "parts": [
              {
                "id": "ts-ac-2_stmt",
                "name": "statement",
                "prose": "Establish and implement highly privileged, audited procedures for obtaining access to ePHI during an emergency."
              },
              {
                "id": "ts-ac-2_guide",
                "name": "guidance",
                "prose": "Define break-glass procedures. Implement emergency access accounts with enhanced auditing. Test procedures annually."
              },
              {
                "id": "ts-ac-2_evidence",
                "name": "evidence",
                "prose": "Emergency access procedures; Break-glass account configurations; Emergency access test records"
              }
            ],
            "links": [
              {
                "href": "#soc2",
                "rel": "related",
