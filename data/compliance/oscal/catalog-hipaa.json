          {
            "id": "ts-ac-4",
            "title": "Encryption and Decryption (at Rest)",
            "props": [
              {
                "name": "implementation-type",
                "value": "addressable"
              },
              {
                "name": "category",
                "value": "Access Control"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-ac-4_stmt",
                "name": "statement",
                "prose": "Implement a mechanism to encrypt and decrypt ePHI stored in databases and storage systems."
              },
              {
                "id": "ts-ac-4_guide",
                "name": "guidance",
                "prose": "Encrypt all ePHI at rest using AES-256. Implement key management procedures. Encrypt database columns containing ePHI."
              },
              {
                "id": "ts-ac-4_evidence",
