          {
            "id": "ts-in-1",
            "title": "Mechanism to Authenticate ePHI",
            "props": [
              {
                "name": "implementation-type",
                "value": "addressable"
              },
              {
                "name": "category",
                "value": "Integrity"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-in-1_stmt",
                "name": "statement",
                "prose": "Implement electronic mechanisms to corroborate that ePHI has not been altered or destroyed in an unauthorized manner."
              },
              {
                "id": "ts-in-1_guide",
                "name": "guidance",
                "prose": "Implement checksums or digital signatures for ePHI. Use database integrity checks. Validate data integrity through reconciliation."
              },
              {
                "id": "ts-in-1_evidence",
                "name": "evidence",
                "prose": "Integrity check mechanisms; Checksum validation logs; Reconciliation reports"
              }
            ],
