          {
            "id": "ts-ac-3",
            "title": "Automatic Logoff",
            "props": [
              {
                "name": "implementation-type",
                "value": "addressable"
              },
              {
                "name": "category",
                "value": "Access Control"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-ac-3_stmt",
                "name": "statement",
                "prose": "Implement electronic procedures that terminate sessions after a predetermined period of inactivity."
              },
              {
                "id": "ts-ac-3_guide",
                "name": "guidance",
                "prose": "Configure session timeouts for all applications handling ePHI. Set idle timeout to 15 minutes max. Force re-authentication after timeout."
              },
              {
                "id": "ts-ac-3_evidence",
                "name": "evidence",
                "prose": "Session timeout configurations; Application settings screenshots"
              }
            ],
