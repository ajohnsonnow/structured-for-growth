          {
            "id": "ts-tx-2",
            "title": "Encryption in Transit",
            "props": [
              {
                "name": "implementation-type",
                "value": "addressable"
              },
              {
                "name": "category",
                "value": "Transmission Security"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-tx-2_stmt",
                "name": "statement",
                "prose": "Implement a mechanism to encrypt ePHI whenever it is transmitted over electronic communications networks, especially public networks."
              },
              {
                "id": "ts-tx-2_guide",
                "name": "guidance",
                "prose": "Enforce TLS 1.2+ for all API endpoints. Use IPsec for VPN connections. Encrypt email containing ePHI. Disable deprecated protocols."
              },
              {
                "id": "ts-tx-2_evidence",
                "name": "evidence",
                "prose": "TLS version configurations; SSL scan results; VPN encryption settings"
              }
