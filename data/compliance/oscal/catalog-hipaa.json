          {
            "id": "ts-au-1",
            "title": "Audit Controls - Hardware/Software Logging",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Audit Controls"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ts-au-1_stmt",
                "name": "statement",
                "prose": "Implement hardware, software, and/or procedural mechanisms that record and examine activity in systems containing or using ePHI."
              },
              {
                "id": "ts-au-1_guide",
                "name": "guidance",
                "prose": "Log all access to ePHI. Record login attempts, data access, modifications, and deletions. Centralize logs in SIEM. Retain for 6+ years."
              },
              {
                "id": "ts-au-1_evidence",
                "name": "evidence",
                "prose": "Logging configurations; Log retention policy; SIEM setup documentation; Sample audit logs"
              }
