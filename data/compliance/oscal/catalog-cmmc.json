          {
            "id": "ia.l2-3.5.7",
            "title": "Password Complexity",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Authentication"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ia.l2-3.5.7_stmt",
                "name": "statement",
                "prose": "Enforce a minimum password complexity and change of characters when new passwords are created."
              },
              {
                "id": "ia.l2-3.5.7_guide",
                "name": "guidance",
                "prose": "Enforce minimum 14 characters. Require complexity (upper, lower, numbers, special). Screen against breach databases. Implement password managers."
              },
              {
                "id": "ia.l2-3.5.7_evidence",
                "name": "evidence",
                "prose": "Password policy; Enforcement configurations; Breach database screening"
              }
            ],
