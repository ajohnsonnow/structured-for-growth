          {
            "id": "ac.l2-3.1.7",
            "title": "Privileged Function Prevention",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Access Control"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ac.l2-3.1.7_stmt",
                "name": "statement",
                "prose": "Prevent non-privileged users from executing privileged functions and audit the execution of such functions."
              },
              {
                "id": "ac.l2-3.1.7_guide",
                "name": "guidance",
                "prose": "Enforce separation between privileged and non-privileged operations. Audit all privileged function executions. Alert on anomalies."
              },
              {
                "id": "ac.l2-3.1.7_evidence",
                "name": "evidence",
                "prose": "Privileged function audit logs; Separation enforcement evidence; Alerting configurations"
              }
            ],
            "links": [
