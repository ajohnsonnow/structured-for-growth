          {
            "id": "ia.l2-3.5.10",
            "title": "Cryptographically-Protected Passwords",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Authentication"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "ia.l2-3.5.10_stmt",
                "name": "statement",
                "prose": "Store and transmit only cryptographically-protected passwords."
              },
              {
                "id": "ia.l2-3.5.10_guide",
                "name": "guidance",
                "prose": "Hash passwords with bcrypt, scrypt, or Argon2. Never store plaintext. Transmit over TLS only. Implement proper salting."
              },
              {
                "id": "ia.l2-3.5.10_evidence",
                "name": "evidence",
                "prose": "Password storage implementation; Hashing algorithm documentation; No plaintext evidence"
              }
