          },
          {
            "name": "control-count",
            "value": "7"
          }
        ],
        "controls": [
          {
            "id": "gdpr-5.1a",
            "title": "Lawfulness, Fairness, and Transparency",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Processing Principles"
              },
              {
                "name": "automation-capability",
                "value": "partial"
              }
            ],
            "parts": [
              {
                "id": "gdpr-5.1a_stmt",
                "name": "statement",
                "prose": "Personal data shall be processed lawfully, fairly and in a transparent manner in relation to the data subject."
              },
              {
                "id": "gdpr-5.1a_guide",
                "name": "guidance",
                "prose": "Document lawful basis for each processing activity. Provide clear privacy notices. Ensure transparency in all data collection."
              },
              {
                "id": "gdpr-5.1a_evidence",
                "name": "evidence",
                "prose": "Privacy policy; Lawful basis documentation; Privacy notices"
              }
            ],
            "links": [
              {
                "href": "#soc2",
                "rel": "related",
                "text": "soc2: P1.1"
              },
              {
                "href": "#hipaa",
                "rel": "related",
                "text": "hipaa: Notice of Privacy Practices"
              }
            ]
          },
          {
            "id": "gdpr-5.1b",
            "title": "Purpose Limitation",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Processing Principles"
              },
              {
                "name": "automation-capability",
                "value": "partial"
              }
            ],
            "parts": [
              {
                "id": "gdpr-5.1b_stmt",
                "name": "statement",
                "prose": "Data shall be collected for specified, explicit and legitimate purposes and not further processed in a manner incompatible with those purposes."
              },
              {
                "id": "gdpr-5.1b_guide",
                "name": "guidance",
                "prose": "Document purpose for each data collection. Prevent function creep. Implement technical controls to restrict data use to stated purposes."
              },
              {
                "id": "gdpr-5.1b_evidence",
                "name": "evidence",
                "prose": "Purpose documentation; Processing register; Technical use restrictions"
              }
            ],
            "links": [
              {
                "href": "#soc2",
                "rel": "related",
                "text": "soc2: P6.1"
              }
            ]
          },
          {
            "id": "gdpr-5.1c",
            "title": "Data Minimization",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Processing Principles"
              },
              {
                "name": "automation-capability",
                "value": "partial"
              }
            ],
            "parts": [
              {
                "id": "gdpr-5.1c_stmt",
                "name": "statement",
                "prose": "Data shall be adequate, relevant and limited to what is necessary in relation to the purposes for which they are processed."
              },
              {
                "id": "gdpr-5.1c_guide",
                "name": "guidance",
                "prose": "Audit data collection forms. Remove unnecessary fields. Validate that only essential data is collected in APIs."
              },
              {
                "id": "gdpr-5.1c_evidence",
                "name": "evidence",
                "prose": "Data minimization review; Form field audits; API payload reviews"
              }
            ],
            "links": [
              {
                "href": "#soc2",
                "rel": "related",
                "text": "soc2: P6.1"
              },
              {
                "href": "#iso27001",
