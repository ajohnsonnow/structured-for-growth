            "parts": [
              {
                "id": "gdpr-5.1f_stmt",
                "name": "statement",
                "prose": "Data shall be processed with appropriate security measures, including protection against unauthorized or unlawful processing and against accidental loss."
              },
              {
                "id": "gdpr-5.1f_guide",
                "name": "guidance",
                "prose": "Implement encryption at rest and in transit. Deploy access controls. Conduct regular security assessments."
              },
              {
                "id": "gdpr-5.1f_evidence",
                "name": "evidence",
                "prose": "Encryption configurations; Access control evidence; Security assessment reports"
