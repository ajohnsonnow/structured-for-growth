            ],
            "parts": [
              {
                "id": "gdpr-5.1d_stmt",
                "name": "statement",
                "prose": "Data shall be accurate and, where necessary, kept up to date."
              },
              {
                "id": "gdpr-5.1d_guide",
                "name": "guidance",
                "prose": "Implement data quality checks. Provide self-service profile updates. Validate data inputs."
              },
              {
                "id": "gdpr-5.1d_evidence",
                "name": "evidence",
                "prose": "Data quality procedures; Update mechanisms; Validation rules"
              }
            ],
            "links": [
              {
                "href": "#soc2",
                "rel": "related",
                "text": "soc2: PI1.1"
              }
            ]
          },
          {
            "id": "gdpr-5.1e",
            "title": "Storage Limitation",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Processing Principles"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "gdpr-5.1e_stmt",
                "name": "statement",
                "prose": "Data shall be kept in a form which permits identification of data subjects for no longer than is necessary."
              },
              {
                "id": "gdpr-5.1e_guide",
                "name": "guidance",
                "prose": "Define retention periods per data category. Implement automated deletion. Archive or anonymize data past retention period."
              },
              {
                "id": "gdpr-5.1e_evidence",
                "name": "evidence",
                "prose": "Retention schedule; Automated deletion evidence; Anonymization procedures"
              }
            ],
