      {
        "id": "data-protection",
        "title": "Protect Account Data",
        "props": [
          {
            "name": "required",
            "value": "true"
          },
          {
            "name": "control-count",
            "value": "2"
          }
        ],
        "controls": [
          {
            "id": "pci-3",
            "title": "Protect Stored Account Data",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Data Protection"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "pci-3_stmt",
                "name": "statement",
                "prose": "Protect stored account data using strong cryptography. PAN must be rendered unreadable anywhere it is stored."
              },
              {
                "id": "pci-3_guide",
                "name": "guidance",
                "prose": "Encrypt PAN using AES-256. Implement tokenization where possible. Never store SAD after authorization. Implement key management procedures."
              },
              {
                "id": "pci-3_evidence",
                "name": "evidence",
                "prose": "Encryption configurations; Tokenization implementation; Key management procedures; SAD disposal evidence"
              }
            ],
