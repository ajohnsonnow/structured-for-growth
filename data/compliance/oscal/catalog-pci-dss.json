          },
          {
            "name": "control-count",
            "value": "2"
          }
        ],
        "controls": [
          {
            "id": "pci-1",
            "title": "Install and Maintain Network Security Controls",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Network Security"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "pci-1_stmt",
                "name": "statement",
                "prose": "Install and maintain network security controls to restrict traffic to and from the Cardholder Data Environment (CDE)."
              },
              {
                "id": "pci-1_guide",
                "name": "guidance",
                "prose": "Deploy firewalls between CDE and untrusted networks. Implement network segmentation. Restrict inbound/outbound traffic to business-necessary flows. Review firewall rules semi-annually."
              },
              {
                "id": "pci-1_evidence",
                "name": "evidence",
                "prose": "Firewall rules; Network diagrams; Segmentation testing results; Rule review records"
              }
            ],
