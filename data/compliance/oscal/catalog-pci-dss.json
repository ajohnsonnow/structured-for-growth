          {
            "id": "pci-4",
            "title": "Protect Cardholder Data with Strong Cryptography During Transmission",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Transmission Security"
              },
              {
                "name": "automation-capability",
                "value": "full"
              }
            ],
            "parts": [
              {
                "id": "pci-4_stmt",
                "name": "statement",
                "prose": "Protect cardholder data with strong cryptography when transmitted over open, public networks."
              },
              {
                "id": "pci-4_guide",
                "name": "guidance",
                "prose": "Enforce TLS 1.2+ (prefer TLS 1.3). Disable deprecated protocols. Never send PAN via unencrypted email. Validate certificates."
              },
              {
                "id": "pci-4_evidence",
                "name": "evidence",
                "prose": "TLS configurations; SSL scan results; Certificate inventory; Deprecated protocol removal"
              }
            ],
