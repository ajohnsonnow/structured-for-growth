        "props": [
          {
            "name": "required",
            "value": "true"
          },
          {
            "name": "control-count",
            "value": "3"
          }
        ],
        "controls": [
          {
            "id": "nis2-23.1",
            "title": "Early Warning (24 hours)",
            "props": [
              {
                "name": "implementation-type",
                "value": "required"
              },
              {
                "name": "category",
                "value": "Incident Reporting"
              },
              {
                "name": "automation-capability",
                "value": "partial"
              }
            ],
            "parts": [
              {
                "id": "nis2-23.1_stmt",
                "name": "statement",
                "prose": "Submit an early warning to the CSIRT or competent authority within 24 hours of becoming aware of a significant incident."
              },
              {
                "id": "nis2-23.1_guide",
                "name": "guidance",
                "prose": "Establish 24-hour early warning procedures. Define what constitutes 'significant'. Prepare early warning template. Establish 24/7 reporting capability."
              },
              {
                "id": "nis2-23.1_evidence",
                "name": "evidence",
                "prose": "Early warning procedure; Template; Contact details for CSIRT; 24/7 capability evidence"
              }
            ],
            "links": [
              {
