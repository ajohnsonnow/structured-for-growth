            SELECT 
                c.id as client_id,
                c.name as client_name,
                c.email as client_email,
                c.company,
                COUNT(m.id) as message_count,
                SUM(CASE WHEN m.read_at IS NULL AND m.direction = 'inbound' THEN 1 ELSE 0 END) as unread_count,
                MAX(m.created_at) as last_message_at,
                (SELECT content FROM messages WHERE client_id = c.id ORDER BY created_at DESC LIMIT 1) as last_message
            FROM clients c
            LEFT JOIN messages m ON c.id = m.client_id
            GROUP BY c.id
            HAVING message_count > 0
            ORDER BY last_message_at DESC
        'Get conversations error:''Failed to get conversations''/unread/count'
            SELECT COUNT(*) as count FROM messages 
            WHERE direction = 'inbound' AND read_at IS NULL
        'Get unread count error:''Failed to get unread count''/client/:clientId'
            SELECT m.*, u.username as sender_name
            FROM messages m
            LEFT JOIN users u ON m.user_id = u.id
            WHERE m.client_id = ?
            ORDER BY m.created_at ASC
        
            UPDATE messages SET read_at = CURRENT_TIMESTAMP 
            WHERE client_id = ? AND direction = 'inbound' AND read_at IS NULL
        'Get client messages error:''Failed to get messages''Message content is required''sent_via''sms''portal'
                INSERT INTO messages (client_id, user_id, direction, subject, content, sent_via)
                VALUES (?, ?, 'outbound', ?, ?, ?)
            'SELECT * FROM messages WHERE id = ?'Sent message to  via '/bulk''client_ids'isArray'Client IDs must be an array'
